import wollok.game.*
import tipito.*
import personaje.*
import juego.*
import keybinds.* 



program ejemplo {
  juego.iniciar()

  game.whenCollideDo(tipito, { elemento =>
    game.say(elemento, "¡Hola, soy un lobo marino! Apreta Z para conocerme.")
