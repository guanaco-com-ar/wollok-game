import wollok.game.*
import tipito.*
import personaje.*
import juego.*



program ejemplo {
  juego.iniciar()

  game.whenCollideDo(tipito, { elemento =>
    game.say(elemento, "¡Hola, soy un lobo marino! Apreta Z para moverme arriba y X para moverme abajo.")
    game.say(tipito, "¡Hola! ¿Deberia confiar en ti?")
  })

  game.start()
}


